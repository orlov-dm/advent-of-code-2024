{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Pick Folder to Run",
        "skipFiles": ["<node_internals>/**"],
        "program": "${workspaceFolder}/dist/${input:folderName}/index.js"
      }
    ],
    "inputs": [
      {
        "id": "folderName",
        "type": "pickString",
        "description": "Pick a folder to run",
        "options": ["1"],
      }
    ]
  }